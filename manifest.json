{
  "manifest_version": 2,

  "name": "YouTube Video List View",
  "version": "0.1.0",
  "description": "Changes YouTube video list from grid view to list view",
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },
  "content_scripts": [
    {
      "css": ["styles.css"],
      "matches": ["https://www.youtube.com/*"]
    }
  ]
}
